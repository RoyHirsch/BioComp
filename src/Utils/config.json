{
    "input_shape": [36 ,4],
    "batch_size": 20,
    "epochs":10,
    "data_format": "NHWC",

    "conv_kernel_size": [3],
    "conv_strides": 1,
    "conv_depth": 3,
    "conv_filters": 32,
    "strides": [1],
    "pool_size":[2],
    "dropout": 0.25,

    "learning_rate":0.001,
    "momentum":0.9,
    "activation1": "relu",
    "activation2": "sigmoid",
    "fc_activation":"softmax",
    "loss": "binary_crossentropy",
    "optimizer": "Adam",
    "pool_strides": 2,
    "regularization_coeff": 0.01

}
